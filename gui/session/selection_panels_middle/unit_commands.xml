<?xml version="1.0" encoding="UTF-8"?>
<!-- Unit Commands -->
<object name="unitCommandPanel"
	size="0 76%-25 100% 76%+25"
	type="image"
>
	<object size="0% 7 100% 100%">
		<repeat count="6">
			<object name="unitCommandButton[n]" hidden="true" style="iconButton" type="button" size="0 0 37 37" tooltip_style="sessionToolTipBottomBold">
				<object name="unitCommandIcon[n]" ghost="true" type="image" style="commandIcon"/>
				<object name="unitCommandCount[n]" ghost="true" style="groupIconsText" type="text"/>
			</object>
		</repeat>
	</object>
</object>
