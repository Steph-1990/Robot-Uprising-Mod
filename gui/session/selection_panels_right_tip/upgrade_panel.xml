<?xml version="1.0" encoding="UTF-8"?>
<object name="unitUpgradePanel"
	size="5 5 100%-5 100%-5"
>
	<object>
		<repeat count="10">
			<object name="unitUpgradeButton[n]" hidden="true" style="iconButton" type="button" size="0 0 52 52" tooltip_style="sessionToolTipBottom">
				<object name="unitUpgradeIcon[n]" type="image" ghost="true" size="3 3 49 49"/>
				<object name="unitUpgradeUpgradeIcon[n]" type="image" ghost="true" size="3 3 49 49" sprite="stretched:session/icons/upgrade.png"/>
				<object name="unitUpgradeProgressSlider[n]" type="image" sprite="queueProgressSlider" ghost="true" size="3 3 49 49" z="20"/>
				<object name="unitUpgradeSelection[n]" hidden="true" type="image" ghost="true" size="3 3 49 49" sprite="stretched:session/icons/corners.png"/>
				<object name="unitUpgradeCount[n]" ghost="true" style="groupIconsText" type="text" z="30"/>
			</object>
		</repeat>
	</object>
</object>
