<?xml version="1.0" encoding="UTF-8"?>
<object name="unitGarrisonPanel"
	size="5 5 100% 100%"
>
	<object>
		<repeat count="12">
			<object name="unitGarrisonButton[n]" hidden="false" style="iconButton" type="button" size="0 0 46 46" tooltip_style="sessionToolTipBottomBold" z="100">
				<object name="unitGarrisonSelection[n]" type="image" ghost="false"/>
				<object name="unitGarrisonIcon[n]" type="image" ghost="true" size="3 3 43 43"/>
				<object name="unitGarrisonCount[n]" ghost="false" style="groupIconsText" type="text"/>
			</object>
		</repeat>
	</object>
	<!-- Health bar of garrisoned units hidden="true" -->
	<object size="-14 1 -7 100%-49" type="image" name="healthGarrison" tooltip_style="sessionToolTip">
		<translatableAttribute id="tooltip">Health</translatableAttribute>
		<object type="image" sprite="barBorder" ghost="true" size="-1 -1 100%+1 100%+1" hidden="true"/>
		<object type="image" sprite="healthBackground" ghost="true" hidden="true"/>
		<object type="image" sprite="healthForeground" ghost="true" name="healthBarGarrison" hidden="true"/>
		<object type="image" sprite="statsBarShaderVertical" ghost="true" hidden="true"/>
	</object>
</object>
